5).astype(int)  # 1 for Sat/Sun\n\n# Paydays: 15th and last day of the month\ntrain[\"is_payday\"] = train.apply(lambda row: int(row[\"day\"] in [15, row[\"date\"].days_in_month]), axis=1)\n\n\n# Handle NaNs in holidays dataset\nholidays[\"transferred\"] = holidays[\"transferred\"].fillna(False)\n\n# Mark holiday effects\ntrain[\"is_holiday\"] = train[\"date\"].isin(holidays[holidays[\"transferred\"] == False][\"date\"]).astype(int)\ntrain[\"is_transferred_holiday\"] = train[\"date\"].isin(holidays[holidays[\"transferred\"] == True][\"date\"]).astype(int)\n\n# Mark earthquake recovery period (April 16, 2016 - June 1, 2016)\ntrain[\"earthquake_recovery\"] = ((train[\"date\"] >= \"2016-04-16\") & (train[\"date\"] <= \"2016-06-01\")).astype(int)\n\n# Fill missing oil prices (forward fill & back fill)\noil[\"dcoilwtico\"] = oil[\"dcoilwtico\"].fillna(method=\"ffill\").fillna(method=\"bfill\")\n\n# Merge Oil Data (Check if 'dcoilwtico' exists)\nif \"dcoilwtico\" in oil.columns:\n    train = train.merge(oil, on=\"date\", how=\"left\")\nelse:\n    print(\"Warning: 'dcoilwtico' column not found in oil dataset!\")\n\n# Create rolling averages for oil price trends (grouped by year to avoid data leakage)\ntrain[\"oil_price_ma7\"] = train.groupby(\"year\")[\"dcoilwtico\"].transform(lambda x: x.rolling(7, min_periods=1).mean())\ntrain[\"oil_price_ma30\"] = train.groupby(\"year\")[\"dcoilwtico\"].transform(lambda x: x.rolling(30, min_periods=1).mean())\n\n# Set date as index\ntrain.set_index('date', inplace=True)\n\n# Fill missing sales values (just in case)\ntrain['sales'].fillna(0, inplace=True)\n\n# Plot sales over time (weekly resampling)\nplt.figure(figsize=(12, 6))\ntrain['sales'].resample('W').sum().plot(title='Weekly Sales Over Time')\nplt.xlabel(\"Date\")\nplt.ylabel(\"Total Sales\")\nplt.grid()\nplt.show()\n","metadata":{"trusted":true,"execution":{"iopub.status.busy":"2025-03-10T07:20:13.780368Z","iopub.execute_input":"2025-03-10T07:20:13.780733Z","iopub.status.idle":"